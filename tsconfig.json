{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@pixelui/*": ["packages/*/src"]
    },
    "target": "es6",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["packages/**/*"]
}
